{
    "name": "JobRobo",
    "version": "1.0",
    "description": "",
    "manifest_version": 3,
    "action": {
        "default_title": " Extension"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "img/*"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "permissions": [
        "activeTab",
        "storage",
        "scripting"
    ],
    "background": {
        "service_worker": "js/background.bundle.js"
    }
}